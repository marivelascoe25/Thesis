@article{tanOperationMechanismOrganic2021,
	title = {Operation mechanism of organic electrochemical transistors as redox chemical transducers},
	volume = {9},
	issn = {2050-7534},
	url = {https://pubs.rsc.org/en/content/articlelanding/2021/tc/d1tc02224e},
	doi = {10.1039/D1TC02224E},
	abstract = {The ability to control the charge density of organic mixed ionic electronic conductors (OMIECs) via reactions with redox-active analytes has enabled applications as electrochemical redox sensors. Their charge density-dependent conductivity can additionally be tuned via charge injection from electrodes, for instance in organic electrochemical transistors (OECTs), where volumetric charging of the OMIEC channel enables excellent transconductance and amplification of low potentials. Recent efforts have combined the chemical detection with the transistor function of OECTs to achieve compact electrochemical sensors. However, these sensors often fall short of the expected amplification performance of OECTs. Here, we investigate the operation mechanism of various OECT architectures to deduce the design principles required to achieve reliable chemical detection and signal amplification. By utilizing a non-polarizable gate electrode and conducting the chemical reaction in a compartment separate from the OECT, the recently developed Reaction Cell OECT achieves reliable modulation of the OECT channel's charge density. This work demonstrates that systematic and rational design of OECT chemical sensors requires understanding the electrochemical processes that result in changes in the potential (charge density) of the channel, the underlying phenomenon behind amplification.},
	language = {en},
	number = {36},
	urldate = {2023-07-24},
	journal = {Journal of Materials Chemistry C},
	author = {Tan, Siew Ting Melissa and Keene, Scott and Giovannitti, Alexander and Melianas, Armantas and Moser, Maximilian and McCulloch, Iain and Salleo, Alberto},
	month = sep,
	year = {2021},
	note = {Publisher: The Royal Society of Chemistry},
	keywords = {/unread},
	pages = {12148--12158},
	file = {Supplementary Information PDF:C\:\\Users\\32456\\Zotero\\storage\\HQICNNDI\\Tan et al. - 2021 - Operation mechanism of organic electrochemical tra.pdf:application/pdf;Tan et al_2021_Operation mechanism of organic electrochemical transistors as redox chemical.pdf:C\:\\Users\\32456\\Zotero\\storage\\SI4AKSN7\\Tan et al_2021_Operation mechanism of organic electrochemical transistors as redox chemical.pdf:application/pdf},
}
@article{Keene_enhmod_pedot,
	author = {Keene, Scott T. and van der Pol, Tom P. A. and Zakhidov, Dante and Weijtens, Christ H. L. and Janssen, René A. J. and Salleo, Alberto and van de Burgt, Yoeri},
	title = {Enhancement-Mode PEDOT:PSS Organic Electrochemical Transistors Using Molecular De-Doping},
	journal = {Advanced Materials},
	volume = {32},
	number = {19},
	pages = {2000270},
	keywords = {aliphatic amines, bioelectronics, enhancement-mode transistor, molecular doping, organic electrochemical transistor, poly(ethylenedioxythiophene):poly(styrene sulfonate},
	doi = {https://doi.org/10.1002/adma.202000270},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/adma.202000270},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/adma.202000270},
	abstract = {Abstract Organic electrochemical transistors (OECTs) show great promise for flexible, low-cost, and low-voltage sensors for aqueous solutions. The majority of OECT devices are made using the polymer blend poly(ethylenedioxythiophene):poly(styrene sulfonate) (PEDOT:PSS), in which PEDOT is intrinsically doped due to inclusion of PSS. Because of this intrinsic doping, PEDOT:PSS OECTs generally operate in depletion mode, which results in a higher power consumption and limits stability. Here, a straightforward method to de-dope PEDOT:PSS using commercially available amine-based molecular de-dopants to achieve stable enhancement-mode OECTs is presented. The enhancement-mode OECTs show mobilities near that of pristine PEDOT:PSS (≈2 cm2 V−1 s−1) with stable operation over 1000 on/off cycles. The electron and proton exchange among PEDOT, PSS, and the molecular de-dopants are characterized to reveal the underlying chemical mechanism of the threshold voltage shift to negative voltages. Finally, the effect of the de-doping on the microstructure of the spin-cast PEDOT:PSS films is investigated.},
	year = {2020}
}