	doi = {10.1038/s41563-018-0263-6},
	abstract = {Molecular doping is a crucial tool for controlling the charge-carrier concentration in organic semiconductors. Each dopant molecule is commonly thought to give rise to only one polaron, leading to a maximum of one donor:acceptor charge-transfer complex and hence an ionization efficiency of 100\%. However, this theoretical limit is rarely achieved because of incomplete charge transfer and the presence of unreacted dopant. Here, we establish that common p-dopants can in fact accept two electrons per molecule from conjugated polymers with a low ionization energy. Each dopant molecule participates in two charge-transfer events, leading to the formation of dopant dianions and an ionization efficiency of up to 200\%. Furthermore, we show that the resulting integer charge-transfer complex can dissociate with an efficiency of up to 170\%. The concept of double doping introduced here may allow the dopant fraction required to optimize charge conduction to be halved.},
	language = {en},
	number = {2},
	urldate = {2023-05-10},
	journal = {Nature Materials},
	author = {Kiefer, David and Kroon, Renee and Hofmann, Anna I. and Sun, Hengda and Liu, Xianjie and Giovannitti, Alexander and Stegerer, Dominik and Cano, Alexander and Hynynen, Jonna and Yu, Liyang and Zhang, Yadong and Nai, Dingqi and Harrelson, Thomas F. and Sommer, Michael and Moulé, Adam J. and Kemerink, Martijn and Marder, Seth R. and McCulloch, Iain and Fahlman, Mats and Fabiano, Simone and Müller, Christian},
	month = feb,
	year = {2019},
	note = {Number: 2
Publisher: Nature Publishing Group},
	keywords = {Conjugated polymers, Electron transfer, Electronic materials, Polymers},
	pages = {149--155},
	file = {Full Text PDF:C\:\\Users\\32456\\Zotero\\storage\\5JFPWGLB\\Kiefer et al. - 2019 - Double doping of conjugated polymers with monomer .pdf:application/pdf},
}

@article{duanHighlyEfficientMixed,
	title = {Highly {Efficient} {Mixed} {Conduction} in a {Fused} {Oligomer} n-{Type} {Organic} {Semiconductor} {Enabled} by {3D} {Transport} {Pathways}},
	volume = {n/a},
	issn = {1521-4095},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/adma.202300252},
	doi = {10.1002/adma.202300252},
	abstract = {Tailoring organic semiconductors to facilitate mixed conduction of ionic and electronic charges when interfaced with an aqueous media has spurred many recent advances in organic bioelectronics. The field is still restricted, however, by very few n-type (electron-transporting) organic semiconductors with adequate performance metrics. Here, a new electron-deficient, fused polycyclic aromatic system, TNR, is reported with excellent n-type mixed conduction properties including a µC* figure-of-merit value exceeding 30 F cm−1 V−1 s−1 for the best performing derivative. Comprising three naphthalene bis-isatin moieties, this new molecular design builds on successful small-molecule mixed conductors; by extending the molecular scaffold into the oligomer domain, good film-forming properties, strong π–π interactions, and consequently excellent charge-transport properties are obtained. Through judicious optimization of the side chains, the linear oligoether and branched alkyl chain derivative bgTNR is obtained which shows superior mixed conduction in an organic electrochemical transistor configuration including an electron mobility around 0.3 cm2 V−1 s−1. By optimizing the side chains, the dominant molecular packing can be changed from a preferential edge-on orientation (with high charge-transport anisotropy) to an oblique orientation that can support 3D transport pathways which in turn ensure highly efficient mixed conduction properties across the bulk semiconductor film.},
	language = {en},
	number = {n/a},
	urldate = {2023-05-10},
	journal = {Advanced Materials},
	author = {Duan, Jiayao and Zhu, Genming and Chen, Junxin and Zhang, Chenyang and Zhu, Xiuyuan and Liao, Hailiang and Li, Zhengke and Hu, Hanlin and McCulloch, Iain and Nielsen, Christian B. and Yue, Wan},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/adma.202300252},
	keywords = {mixed conductors, n-type semiconductors, oligomers, organic bioelectronics, organic electrochemical transistors},
	pages = {2300252},
	file = {Full Text PDF:C\:\\Users\\32456\\Zotero\\storage\\N5E8VI73\\Duan et al. - Highly Efficient Mixed Conduction in a Fused Oligo.pdf:application/pdf;Snapshot:C\:\\Users\\32456\\Zotero\\storage\\M7JWFMJP\\adma.html:text/html},
}

@article{quillOrderedSelfassembledNanocomposite2023,
	title = {An ordered, self-assembled nanocomposite with efficient electronic and ionic transport},
	volume = {22},
