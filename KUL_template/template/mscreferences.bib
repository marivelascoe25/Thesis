@article{kleemannStructuralPhaseTransition2012,
  title = {Structural Phase Transition in Pentacene Caused by Molecular Doping and Its Effect on Charge Carrier Mobility},
  author = {Kleemann, Hans and Schuenemann, Christoph and Zakhidov, Alexander A. and Riede, Moritz and L{\"u}ssem, Bj{\"o}rn and Leo, Karl},
  year = {2012},
  month = jan,
  journal = {Organic Electronics},
  volume = {13},
