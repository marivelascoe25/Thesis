@article{amdurskyMacroscaleBiomolecularElectronics2019,
  title = {Macroscale {{Biomolecular Electronics}} and {{Ionics}}},
  author = {Amdursky, Nadav and G{\l}owacki, Eric Daniel and Meredith, Paul},
  year = {2019},
  journal = {Advanced Materials},
  volume = {31},
  number = {3},
  pages = {1802221},
  issn = {1521-4095},
  doi = {10.1002/adma.201802221},
  urldate = {2023-08-10},
  abstract = {The conduction of ions and electrons over multiple length scales is central to the processes that drive the biological world. The multidisciplinary attempts to elucidate the physics and chemistry of electron, proton, and ion transfer in biological charge transfer have focused primarily on the nano- and microscales. However, recently significant progress has been made on biomolecular materials that can support ion and electron currents over millimeters if not centimeters. Likewise, similar transport phenomena in organic semiconductors and ionics have led to new innovations in a wide variety of applications from energy generation and storage to displays and bioelectronics. Here, the underlying principles of conduction on the macroscale in biomolecular materials are discussed, highlighting recent examples, and particularly the establishment of accurate structure\textendash property relationships to guide rationale material and device design. The technological viability of biomolecular electronics and ionics is also discussed.},
  copyright = {\textcopyright{} 2018 WILEY-VCH Verlag GmbH \& Co. KGaA, Weinheim},
  langid = {english},
  keywords = {bioelectronics,biomaterials,conductive polymers,electron conduction,proton conduction},
  file = {C\:\\Users\\menriquez\\Zotero\\storage\\QXUSZ7RG\\Amdursky et al_2019_Macroscale Biomolecular Electronics and Ionics.pdf;C\:\\Users\\menriquez\\Zotero\\storage\\8CC33WQT\\adma.html}
}

@phdthesis{barkerConjugatedPolymerBlends2021,
  title = {Conjugated Polymer Blends for Faster Organic Mixed Ionic-Electronic Conductors},
  author = {Barker, Micah},
  year = {2021},
  month = dec,
  abstract = {The operation of various organic electrochemical devices relies on the intrinsic mixed conducting properties of {$\pi$}-conjugated polymers. One key factor that generally limits device performance is the slow transport of ions throughout the semiconducting polymer, necessary for bulk doping and dedoping. Thus, there has been increasing efforts to enhance the ionic conductivity of various conjugated polymers by chemically engineering the polarity of the side chains of the material. Such substituents however can impact the structure of these materials and render their electronic properties sensitive to environmental conditions. This work demonstrates the viability of multicomponent polymer blending as an alternative approach to tune mixed conductivity. A hydrophilic block copolymer of poly(3-hexylthiophene) and poly(ethylene oxide) (P3HT-b-PEO) is blended with a mixed conducting random copolymer of P3HT and poly(3-hydroxyhexylthiophene) (P3HT-co-P3HHT). Electrochemical transistors of the blends exhibit similar transduction performance at steady state relative to those of the neat P3HT-co-P3HHT, however the response time is greatly reduced. We attribute this to faster electrochemical doping related to ion uptake due to the increased material hydrophilicity upon adding the block copolymer. Meanwhile, the P3HTmoiety of the block copolymer seems to act as a compatibilizer that can support the structural stability of the blend, which can subsequently support the operational stability of resulting devices. Thus, our work implies that engineering block copolymers to fabricate compatible blends is a promising method to enhance the performance and stability of mixed conducting materials.},
  langid = {english},
  school = {Universit\'e de Bordeaux},
  file = {C\:\\Users\\menriquez\\Zotero\\storage\\FL98ZQKW\\Barker - Conjugated polymer blends for faster organic mixed.pdf}
}

@article{bernardsSteadyStateTransientBehavior2007,
  title = {Steady-{{State}} and {{Transient Behavior}} of {{Organic Electrochemical Transistors}}},
  author = {Bernards, D. A. and Malliaras, G. G.},
  year = {2007},
  journal = {Advanced Functional Materials},
  volume = {17},
  number = {17},
  pages = {3538--3544},
  issn = {1616-3028},
  doi = {10.1002/adfm.200601239},
  urldate = {2023-08-02},
  abstract = {In recent years, organic electrochemical transistors (OECTs) have emerged as attractive devices for a variety of applications, particularly in the area of sensing. While the electrical characteristics of OECTs are analogous to those of conventional organic field effect transistors, appropriate models for OECTs have not yet been developed. In particular, little is known about the transient characteristics of OECTs, which are determined by a complex interplay between ionic and electronic motion. In this paper a simple model is presented that reproduces the steady-state and transient response of OECTs by considering these devices in terms of an ionic and an electronic circuit. A simple analytical expression is derived that can be used to fit steady-state OECT characteristics. For the transient regime, comparison with experimental data allowed an estimation of the hole mobility in poly(3,4-ethylenedioxythiophene) doped with poly(styrene sulfonate). This work paves the way for rational optimization of OECTs.},
  langid = {english},
  keywords = {/unread,Biosensors,Conducting polymers,Sensors,Transistors},
  file = {C\:\\Users\\menriquez\\Zotero\\storage\\VW4XWC8V\\Bernards_Malliaras_2007_Steady-State and Transient Behavior of Organic Electrochemical Transistors.pdf;C\:\\Users\\menriquez\\Zotero\\storage\\GA47DXEL\\adfm.html}
}

@mastersthesis{bongartzOrganicElectrochemicalTransistors2021,
  title = {Organic Electrochemical Transistors with Solid-State Electrolytes},
  author = {Bongartz, Lukas Matthias},
  year = {2021},
  month = jul,
  address = {{Dresden, Germany}},
  abstract = {Organic electrochemical transistors (OECTs) represent a class of electronic devices that received increasing attention in recent years. Their unique working mechanism, which is defined by the interplay of electronic and ionic charge carriers, gives them in inherent similarity to biological synapses and thus renders the devices highly appealing for neuromorphic computing. While OECTs reported in literature are mostly employing aqueous electrolytes, this work focuses on newly developed systems with solid-state electrolytes, which are based on an ionic liquid and that should greatly facilitate applications. This thesis presents very promising electrical characteristics for these novel devices in terms of low threshold voltages ({$<$} 0.3V), high on/off-ratios ({$\sim$} 105), and maximum transconductance around 0V. Transfer curves were also found to exhibit remarkably large hysteresis, which is typically a drawback of transistor devices. However, here it represents an attractive feature for the particular applications sought. On the downside though, the devices also feature channel degradation and irreproducibility under ambient conditions. A new quantification method for hysteresis is introduced that allows to compare its relative extent through a parameter {$\psi$}, which also denotes the progress direction through its sign. Interface experiments indicate that the commonly recognised understanding of OECTs cannot be directly transferred to systems with solid-state electrolytes, of which the electrical characteristics could be rendered reproducible under inert conditions. This prerequisite allowed to identify hysteresis as a constant and reliable property of transfer characteristics, which appears to saturate above a certain gate bias and may potentially be based on coexisting equilibrium states. The suspicion that different mechanisms than originally assumed are involved was further substantiated by experiments with modified channels and exchanged electrolytes. These studies revealed that not only is the ionic liquid not decisive for the gating process, but moreover that external ion sources in general are not required for operating the OECT. It is suspected that the underlying mechanisms are based on inherently included sodium ions in the channel material PEDOT:PSS, which were proven to be approximately a hundred times higher concentrated as specified by the supplier ({$>$} 7000 ppm found). This insight allowed to interpret the outcome of investigations on the potential drop in PEDOT:PSS under various electrolytes. The comparison of devices with altered channel thicknesses revealed that different response times account for different extents of hysteresis, which was confirmed by its decrease for slower scan rates. However, it was also found that the hysteresis cannot be completely suppressed, which is considered a strong indicator of a bistable equilibrium state based on other than purely kinetic processes.},
  langid = {english},
  school = {Technische Universit\"at Dresden},
  file = {C\:\\Users\\menriquez\\Zotero\\storage\\3IB58MLS\\Master_Thesis_Lukas_Bongartz.pdf}
}

