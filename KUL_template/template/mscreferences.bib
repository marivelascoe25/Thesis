}

@article{kochDevicePhysicsModeling2023,
  title = {Device {{Physics}}, {{Modeling}} and {{Simulation}} of {{Organic Electrochemical Transistors}}},
  author = {Koch, Malte and Tseng, Hsin and Weissbach, Anton and Iniguez, Benjamin and Leo, Karl and Kloes, Alexander and Kleemann, Hans and Darbandy, Ghader},
