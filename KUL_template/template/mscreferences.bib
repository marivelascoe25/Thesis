@mastersthesis{buchholtzDopingPropertiesNovel2021,
  title = {Doping {{Properties}} of {{Novel Dopants}} for {{Organic Hole Transport Layers}}},
  author = {Buchholtz, Stephanie Anna},
  year = {2021},
  month = {nov},
  address = {{Dresden, Germany}},
  abstract = {A new class of dopant materials for organic hole transport layers, developed by the TU-Dresden spin-off project Credoxys, is investigated in this thesis. These dopants show an advantage regarding the absorption in the visible and near infrared range. Here, no parasitic absorption by dopant radical anions is detected. The focus is to study the doping efficiency and absorption of thin films doped with these materials. The mainly utilized host material was MeO-TPD, which is a common hole transport material. F6-TCNNQ serves as reference dopant, since it is an efficient dopant. Both materials are well-characterized in several scientific studies. As representative of the new materials, a prototype dopant (CDX-A) and a further developed dopant (CDX-B), are investigated. It is shown that both dopants are capable of doping MeO-TPD. Thereby, conductivity values of up to 2.25 {$\cdot$} 10-4 S/cm are achieved for MeO-TPD:CDX-B at a dopant concentration of 8 mol\%, while MeO-TPD:F6-TCNNQ exhibits only 1.66 {$\cdot$} 10-4 S/cm. By using impedance spectroscopy, doping efficiencies of 0.17 and 0.66 are determined for CDX-A and CDX-B in MeO-TPD at a dopant concentration of 1.7 mol\%. Thus, CDX-B yields a slightly worse result than F6-TCNNQ, which has a doping efficiency of 0.79 at the same dopant concentration. Because of additional trap states in the layers doped by CDX-A, which is detected with impedance spectroscopy, infrared spectroscopy is performed. The aim is to clarify whether the ligands of CDX-A are separated during the doping process, which could be the origin of the additional trap states. However, it is not possible to confirm this theory, since the spectrum of the ligands is largely consistent with that of CDX-A. Finally, also ultraviolet photoelectron spectroscopy of MeO-TPD doped by CDX-A is made. A shift in the chemical potential is confirmed.},
  langid = {english},
  school = {Technische Universit\"at Dresden},
  file = {C\:\\Users\\menriquez\\Zotero\\storage\\LRCZDXEA\\Buchholtz_Stephanie_Anna_Masterarbeit_2021.pdf}
}

@article{cendraRoleAnionTransport2019,
  title = {Role of the {{Anion}} on the {{Transport}} and {{Structure}} of {{Organic Mixed Conductors}}},
  author = {Cendra, Camila and Giovannitti, Alexander and Savva, Achilleas and Venkatraman, Vishak and McCulloch, Iain and Salleo, Alberto and Inal, Sahika and Rivnay, Jonathan},
  year = {2019},
  journal = {Advanced Functional Materials},
  volume = {29},
  number = {5},
  pages = {1807034},
  issn = {1616-3028},
  doi = {10.1002/adfm.201807034},
  urldate = {2023-08-06},
  abstract = {Organic mixed conductors are increasingly employed in electrochemical devices operating in aqueous solutions that leverage simultaneous transport of ions and electrons. Indeed, their mode of operation relies on changing their doping (oxidation) state by the migration of ions to compensate for electronic charges. Nevertheless, the structural and morphological changes that organic mixed conductors experience when ions and water penetrate the material are not fully understood. Through a combination of electrochemical, gravimetric, and structural characterization, the effects of water and anions with a hydrophilic conjugated polymer are elucidated. Using a series of sodium-ion aqueous salts of varying anion size, hydration shells, and acidity, the links between the nature of the anion and the transport and structural properties of the polymer are systematically studied. Upon doping, ions intercalate in the crystallites, permanently modifying the lattice spacings, and residual water swells the film. The polymer, however, maintains electrochemical reversibility. The performance of electrochemical transistors reveals that doping with larger, less hydrated, anions increases their transconductance but decreases switching speed. This study highlights the complexity of electrolyte-mixed conductor interactions and advances materials design, emphasizing the coupled role of polymer and electrolyte (solvent and ion) in device performance.},
  copyright = {\textcopyright{} 2018 WILEY-VCH Verlag GmbH \& Co. KGaA, Weinheim},
  langid = {english},
  keywords = {bioelectronics,doping,organic mixed conductors,structure\textendash property relationships},
  file = {C\:\\Users\\menriquez\\Zotero\\storage\\285WPXP2\\Cendra et al. - 2019 - Role of the Anion on the Transport and Structure o.pdf;C\:\\Users\\menriquez\\Zotero\\storage\\R5789XIU\\adfm.html}
}

