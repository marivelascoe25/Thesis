  file = {C\:\\Users\\menriquez\\Zotero\\storage\\LSEY4IUB\\Tietze et al_2012_Fermi level shift and doping efficiency in $p$-doped small molecule organic.pdf;C\:\\Users\\menriquez\\Zotero\\storage\\278PXGSD\\PhysRevB.86.html}
}

